{
  "name": "reaction-comments",
  "version": "1.0.0",
  "description": "A GitHub App built with Probot that deletes +1 comments.",
  "author": "Armin Sebastian",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/dessant/reaction-comments.git",
    "type": "git"
  },
  "bugs": "https://github.com/dessant/reaction-comments/issues",
  "main": "src/index.js",
  "scripts": {
    "start": "probot run ./src/index.js",
    "watch": "nodemon --exec 'yarn run start'",
    "update": "ncu --upgrade && yarn",
    "push": "git push --follow-tags origin master",
    "release": "standard-version"
  },
  "files": [
    "docs",
    "src"
  ],
  "dependencies": {
    "dedent": "^0.7.0",
    "firebase-admin": "^8.0.0",
    "joi": "^14.3.1",
    "probot": "^9.2.13",
    "probot-config": "^1.0.1",
    "probot-messages": "^1.0.1",
    "probot-scheduler": "^2.0.0-beta.1",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "nodemon": "1.19.1",
    "npm-check-updates": "^3.1.10",
    "prettier": "^1.18.2",
    "smee-client": "^1.1.0",
    "standard-version": "^6.0.1"
  },
  "engines": {
    "node": ">=8.12.0"
  },
  "keywords": [
    "github",
    "issues",
    "pull requests",
    "+1",
    "comments",
    "reactions",
    "github reactions",
    "automation",
    "github app",
    "probot",
    "probot app",
    "bot"
  ]
}
